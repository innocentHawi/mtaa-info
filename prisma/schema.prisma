datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Faq {
  id        Int      @id @default(autoincrement())
  question  String
  answer    String
  embedding Unsupported("vector(1536)")?
  createdAt DateTime @default(now())
}
